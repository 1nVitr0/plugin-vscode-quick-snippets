{
  "name": "quick-snippets",
  "displayName": "Quick Snippets",
  "description": "Copy and paste your clipboard as vscode snippets",
  "author": "1nVitr0",
  "publisher": "1nVitr0",
  "repository": {
    "type": "git",
    "url": "https://github.com/1nVitr0/plugin-vscode-quick-snipet.git"
  },
  "icon": "resources/icon.png",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Data Science",
    "Other"
  ],
  "keywords": [
    "random",
    "sample",
    "pick",
    "random lines",
    "sampling"
  ],
  "activationEvents": [
    "onCommand:quick-snippets.copyToSnippet",
    "onCommand:quick-snippets.copyEditToSnippet",
    "onCommand:quick-snippets.pasteAsSnippet",
    "onCommand:quick-snippets.pasteEditAsSnippet",
    "onCommand:quick-snippets.pasteClipboardSnippet",
    "onCommand:quick-snippets.pasteEditedSnippet",
    "onCommand:quick-snippets.pasteSelectClipboardSnippet",
    "onCommand:quick-snippets.pasteSelectEditedSnippet",
    "onCommand:quick-snippets.editSnippetClipboard",
    "onCommand:quick-snippets.editClipboardAsSnippet",
    "onCommand:quick-snippets.applySelectionTemplates",
    "onCommand:quick-snippets.clearSnippetClipboard"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-snippets.copyToSnippet",
        "title": "Copy as snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.copyEditToSnippet",
        "title": "Edit and Copy as Snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.pasteAsSnippet",
        "title": "Paste as Snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.pasteEditAsSnippet",
        "title": "Edit and Paste as Snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.pasteClipboardSnippet",
        "title": "Paste last Snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.pasteEditedSnippet",
        "title": "Edit and Paste last snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.pasteSelectClipboardSnippet",
        "title": "Paste a previous Snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.pasteSelectEditedSnippet",
        "title": "Edit and Paste a previous snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.editSnippetClipboard",
        "title": "Edit last Snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.editClipboardAsSnippet",
        "title": "Edit and Copy as Snippet",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.applySelectionTemplates",
        "title": "Convert Selection to Templates",
        "category": "Quick Snippet"
      },
      {
        "command": "quick-snippets.clearSnippetClipboard",
        "title": "Clear Snippet Clipboard",
        "category": "Quick Snippet"
      }
    ],
    "configuration": []
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "@types/vscode": "1.72",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "glob": "^8.0.1",
    "mocha": "^10.0.0",
    "semantic-release": "^19.0.2",
    "semantic-release-vsce": "^5.0.11",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "vscode-test": "^1.6.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "node-fetch": "^3.2.10"
  }
}